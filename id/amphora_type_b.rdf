    <kon:Shape rdf:about="http://kerameikos.org/id/amphora_type_b">
        <skos:prefLabel xml:lang="en">Amphora Type B</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">Amphora Type B has a flaring lip with straight or slightly concave sides, an echinus foot, and cylindrical handles. It one of the oldest shapes, created between the late seventh to about 425 B.C.</skos:definition>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
