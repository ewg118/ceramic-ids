        <dcterms:source rdf:resource="http://www.worldcat.org/oclc/38179875"/>
        <rdfs:seeAlso rdf:resource="http://www.perseus.tufts.edu/hopper/text?doc=Perseus:text:1999.04.0004:id=askos"/>
        <rdfs:seeAlso rdf:resource="http://www.beazley.ox.ac.uk/tools/pottery/shapes/askos.htm"/>
        <skos:exactMatch rdf:resource="http://dbpedia.org/resource/Askos_(pottery_vessel)"/>
    </kon:Shape>
