    <kon:Shape rdf:about="http://kerameikos.org/id/askos">
        <skos:prefLabel xml:lang="en">Askos</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Askos</skos:prefLabel>
        <skos:prefLabel xml:lang="pt">Asco (vaso)</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Askos</skos:prefLabel>
        <skos:prefLabel xml:lang="es">Askos</skos:prefLabel>
        <skos:prefLabel xml:lang="ru">Аскос</skos:prefLabel>
        <skos:prefLabel xml:lang="fr">Askos</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">An askos is a small, shallow vessel with a domed top, a short spout angled from one side of the top, and an overarching handle. It could either be wheel-made or moldmade. The vessel was used for oil, but the shape could have also contained perfume, honey, or wine for funerary libations. It was a red-figure vase made from the early fifth to the fourth century B.C.</skos:definition>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
