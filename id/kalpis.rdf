    <kon:Shape rdf:about="http://kerameikos.org/id/kalpis">
        <skos:prefLabel xml:lang="en">Kalpis</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">The so-called Kalpis or Continuous-curve Hydria differs from the Shoulder Hydria in several ways: the neck, shoulder, and body form a continuous curve; its vertical handle is cylindrical and is rooted on the neck rather than on the lip; its foot shape is more varied; its rim is concave on top; and it is usually smaller. It was popular between the end of the sixth through the fourth century B.C.</skos:definition>
        <skos:inScheme rdf:resource="http://kerameikos.org/id/"/>
        <skos:altLabel xml:lang="en">Continuous-curve Hydria</skos:altLabel>
        <dcterms:source rdf:resource="http://www.worldcat.org/oclc/38179875"/>
        <skos:exactMatch rdf:resource="http://collection.britishmuseum.org/id/thesauri/x7460"/>
        <skos:exactMatch rdf:resource="http://vocab.getty.edu/aat/300198846"/>
        <skos:broader rdf:resource="http://kerameikos.org/id/hydria"/>
        <rdfs:seeAlso rdf:resource="http://www.perseus.tufts.edu/hopper/text?doc=Perseus:text:1999.04.0004:id=kalpis"/>
    </kon:Shape>
